{
  "name": "recipe-save-worker",
  "version": "1.0.0",
  "description": "Cloudflare Worker with Durable Object for atomic recipe saves to KV store and search database synchronization",
  "main": "src/index.js",
  "type": "module",
  "scripts": {
    "dev": "wrangler dev",
    "deploy": "wrangler deploy",
    "deploy:preview": "wrangler deploy --env preview",
    "deploy:production": "wrangler deploy --env production",
    "test": "node tests/run-tests.js",
    "test:coverage": "c8 npm test",
    "tail": "wrangler tail",
    "tail:preview": "wrangler tail --env preview",
    "tail:production": "wrangler tail --env production"
  },
  "keywords": [
    "cloudflare-worker",
    "durable-objects",
    "kv-storage",
    "recipe-management"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20240821.1",
    "wrangler": "^3.72.0",
    "c8": "^10.1.2"
  },
  "dependencies": {
    "pako": "^2.1.0"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}